---
// Components
// - Layout
import Layout from '../layouts/Layout.astro'
// - UI
import Hero from '../components/blocks/hero/PageHeader.astro'
import FAQ from '../components/blocks/FAQ/FaqSticky.astro'
import CTA from '../components/blocks/CTA/BasicDark.astro'

// - SEO
const SEO = {
	title: '留学常见问题解答 | 予果予实（EDUGROW）',
	description:
		'关于留学规划、申请流程、文书准备、签证办理、海外适应等常见问题，予果予实（EDUGROW）专业顾问团队为您提供详细解答，解除留学疑虑，助力学业规划。',
}
// - Page Header
const header = {
	title: '予果予实留学常见问题解答',
	text: '关于服务、流程、团队、申请等问题，尽在此处。'
}
// Data
import faqData from '../data/json-files/faqData.json'
---

<Layout title={SEO.title} description={SEO.description}>
	<Hero title={header.title} text={header.text} />
	<FAQ
		title="常见问题"
		text="如有更多疑问，欢迎随时联系我们的顾问团队。"
		data={faqData}
	/>
	<CTA />
</Layout>
